{
    "extends": [
        "eslint:recommended",
        "google",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "prettier",
        "googleappsscript"
    ],
    "env": {
        "es6": true,
        "googleappsscript/googleappsscript": true
    },
    "rules": {
        "prettier/prettier": [
          "error",
          {
            "singleQuote": true,
            "endOfLine": "auto"
          }
        ],
        "no-var": "off",
        "valid-jsdoc": "off",
        "require-jsdoc": "off"
    }
}